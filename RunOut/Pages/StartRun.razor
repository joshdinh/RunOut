@page "/startrun"

<div class="content">
    <div class="nav">
        <button id="back"> &#10094;</button>
        <p id="heading">Walk 1/8</p>
        <div class="date">
            <p id="number">3</p>
            <button disabled id="weekday">Thu</button>
        </div>

    </div>

    <div class="belowNav">
        <div class="nextNotif">
            <p>Next Up</p>
            <h1>RUN</h1>
        </div>

        <div class="stopwatch">
            <h1>00 : 49</h1>
        </div>

        <div class="stats">
            <p id="statsHeading">OVERVIEW</p>
            <div id="distance">
               <div>
                    <h2>Distance</h2>
                    <p class="target">Target: 10km</p>
                </div>
                <p class="number">5</p>
                <p class="km">KM</p>
               
            </div>
            <div id="pace">
                <div>
                    <h2>Average Pace</h2>
                    <p class="target">Target: 5:30/km</p>
                </div>
                <p class="number">6:09</p>
                <p class="km">/KM</p>
                
            </div>
            <button id="overview">OVERVIEW</button>
            <button id="start">START</button>
        </div>
    </div>
 </div>


